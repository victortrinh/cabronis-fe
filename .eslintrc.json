{
  "env": {
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "google",
    "prettier",
    "prettier/react"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react", "@typescript-eslint"],
  "rules": {
    "react/prop-types": 0,
    "require-jsdoc" : 0,
    "camelcase": 0,
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": "error"
  }
}
